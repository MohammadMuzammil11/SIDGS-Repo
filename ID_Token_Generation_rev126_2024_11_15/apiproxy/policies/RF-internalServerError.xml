<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RF-internalServerError">
    <DisplayName>RF-internalServerError</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Payload contentType="application/json" variablePrefix="@@" variableSuffix="##">{"Id-token-jwt": "","Status": "TH99500: Backend Service Provided Unexpected Response"}</Payload>
            <StatusCode>500</StatusCode>
            <ReasonPhrase>Internal Server Error</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>